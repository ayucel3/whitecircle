{
  "name": "whitecircle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n \"frontend,pii-service\" -c \"cyan,magenta\" \"pnpm --filter frontend dev\" \"pnpm --filter pii-service dev\"",
    "build": "pnpm --filter frontend build && pnpm --filter pii-service build",
    "start": "concurrently -n \"frontend,pii-service\" -c \"cyan,magenta\" \"pnpm --filter frontend start\" \"pnpm --filter pii-service start\"",
    "db:migrate": "pnpm --filter frontend prisma migrate dev",
    "db:studio": "pnpm --filter frontend prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.14.2",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
